{
  "name": "ugla",
  "version": "1.4.18",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "sass-lint": "node_modules/sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "lint": "ng lint --format=stylish --type-check true && npm run sass-lint",
    "e2e": "ng e2e",
    "preinstall": "npm install js-datepicker ngx-filepond tooltip.js",
    "deploy:site": "ng build && firebase deploy",
    "compodoc:s": "node_modules/@compodoc/compodoc/bin/index-cli.js -p projects/ugla/tsconfig.lib.json -s --theme material",
    "build-lib": "ng-packagr -p projects/ugla/package.json && cp -r projects/ugla/src/sass/ dist/ && cd dist && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ciandt/ugla.git"
  },
  "bugs": {
    "url": "https://github.com/ciandt/ugla/issues"
  },
  "keywords": [
    "angular",
    "javascript",
    "typescript",
    "components",
    "library",
    "scss"
  ],
  "homepage": "https://ugla.dev",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "core-js": "^2.5.4",
    "filepond": "^4.4.3",
    "filepond-plugin-file-validate-size": "^2.1.1",
    "filepond-plugin-file-validate-type": "^1.2.2",
    "filepond-plugin-image-crop": "^2.0.1",
    "filepond-plugin-image-exif-orientation": "^1.0.4",
    "filepond-plugin-image-preview": "^4.0.1",
    "filepond-plugin-image-resize": "^2.0.2",
    "filepond-plugin-image-transform": "^3.2.2",
    "js-datepicker": "^4.6.6",
    "moment": "^2.24.0",
    "ngx-filepond": "^5.0.1",
    "react-navigation-stack": "^2.1.1",
    "rxjs": "~6.3.3",
    "tooltip.js": "^1.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.7",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "eslint": ">=4.18.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "node-sass": "^4.13.1",
    "protractor": "~5.4.0",
    "sass-lint": "^1.13.1",
    "serialize-javascript": "^2.1.2",
    "set-value": ">=2.0.1",
    "tar": "^4.4.8",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "^5.17.0",
    "tslint-angular": "^3.0.2",
    "typescript": "~3.2.2"
  },
  "main": "dist/ugla/ugla.d.ts",
  "directories": {
    "lib": "dist/*"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=10.0.0"
  }
}
